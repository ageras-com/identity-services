{
  "name": "migrations",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm migration:create ./src/$npm_config_name",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/datasource.ts ./src/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run -d ./src/datasource.ts"
  },
  "dependencies": {
    "@identity-services/common": "*",
    "@identity-services/eslint-config": "*",
    "@identity-services/typescript-config": "*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "typescript-eslint": "8.20.0",
    "eslint-plugin-prettier": "5.2.2"
  }
}
