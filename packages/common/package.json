{
  "name": "@identity-services/common",
  "version": "0.0.0",
  "description": "",
  "main": "main/dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./*": {
      "require": "./dist/*/index.js",
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsc --watch --preserveWatchOutput",
    "build": "tsc",
    "lint": "eslint ./src --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "check:types": "tsc --noEmit"
  },
  "private": true,
  "peerDependencies": {
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.0.1",
    "@nestjs/core": "11.0.1",
    "@nestjs/cqrs": "11.0.3",
    "@nestjs/swagger": "11.1.5",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/class-validator": "0.13.4",
    "class-transformer": "0.5.1",
    "jsonwebtoken": "9.0.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.24",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@identity-services/eslint-config": "*",
    "@identity-services/typescript-config": "*",
    "@types/node": "22.10.7",
    "eslint": "9.30.1",
    "typescript": "5.8.2"
  }
}
